{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "@vercel/react-router",
  "installCommand": "npm install",
  "functions": {
    "build/server/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/build/server/index.js"
    }
  ]
}
